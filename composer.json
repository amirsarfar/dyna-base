{
    "name": "amirsarfar/dyna-base",
    "description": "A headless CMS designed to create dynamically defined data models and API endpoints",
    "license": "MIT",
    "authors": [
        {
            "name": "Amir Sarfar",
            "email": "amirsarfar@gmail.com",
            "homepage": "http://amirsarfarazi.ir"
        }
    ],
    "homepage": "https://github.com/amirsarfar/dyna-base",
    "keywords": ["Laravel", "DynaBase", "dyna", "headless CMS"],
    "require": {
        "illuminate/support": "~7|~8",
        "web64/laravel-cli-colors": "^0.20.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "files": [
            "src/autoload.php"
        ],
        "psr-4": {
            "Amirsarfar\\DynaBase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amirsarfar\\DynaBase\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Amirsarfar\\DynaBase\\DynaBaseServiceProvider"
            ],
            "aliases": {
                "DynaBase": "Amirsarfar\\DynaBase\\Facades\\DynaBase"
            }
        }
    }
}
